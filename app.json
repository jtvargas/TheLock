{
  "expo": {
    "name": "The Lock",
    "slug": "TheLock",
    "version": "4.5",
    "orientation": "portrait",
    "icon": "./assets/v2.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash-v4.png",
